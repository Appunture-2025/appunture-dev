{
  "expo": {
    "name": "Appunture",
    "slug": "appunture",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#007aff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.appunture.mobile"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#007aff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.appunture.mobile"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite",
      "expo-secure-store",
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "scheme": "appunture",
    "extra": {
      "apiBaseUrl": "http://localhost:8080/api",
      "firebaseApiKey": "your-firebase-api-key",
      "firebaseAuthDomain": "your-project.firebaseapp.com",
      "firebaseProjectId": "your-firebase-project-id",
      "firebaseStorageBucket": "your-project.appspot.com",
      "firebaseMessagingSenderId": "your-sender-id",
      "firebaseAppId": "your-firebase-app-id",
      "firebaseMeasurementId": "your-measurement-id",
      "firebaseDatabaseUrl": "https://your-project.firebaseio.com",
      "enableStorageUpload": true,
      "router": {},
      "eas": {
        "projectId": "864e35bc-9ecf-4e7a-866b-8507d1163044"
      }
    }
  }
}
