{
  "title": "Appunture Sync & Storage",
  "uid": "appunture-sync-storage",
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 1,
  "refresh": "30s",
  "tags": ["appunture", "sync", "storage", "mobile"],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "panels": [
    {
      "type": "stat",
      "title": "Auth Sync Success Rate",
      "id": 1,
      "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=\"/auth/sync\", status=\"200\"}[5m])) / sum(rate(http_server_requests_seconds_count{uri=\"/auth/sync\"}[5m])) * 100",
          "legendFormat": "Success %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 90 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "type": "stat",
      "title": "Storage Upload Success Rate",
      "id": 2,
      "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/storage/.*\", method=\"POST\", status=\"200\"}[5m])) / sum(rate(http_server_requests_seconds_count{uri=~\"/storage/.*\", method=\"POST\"}[5m])) * 100",
          "legendFormat": "Success %"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": null },
              { "color": "orange", "value": 90 },
              { "color": "green", "value": 99 }
            ]
          }
        }
      },
      "options": {
        "reduceOptions": {
          "calcs": ["lastNotNull"]
        }
      }
    },
    {
      "type": "gauge",
      "title": "Offline Queue Size (Simulated)",
      "id": 3,
      "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "app_offline_queue_size or vector(0)",
          "legendFormat": "Queue Size"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0,
          "max": 50,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 5 },
              { "color": "red", "value": 10 }
            ]
          }
        }
      }
    },
    {
      "type": "stat",
      "title": "Active Users (Last Hour)",
      "id": 4,
      "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "count(count by (user) (http_server_requests_seconds_count{uri=~\"/auth/.*\"})) or vector(0)",
          "legendFormat": "Users"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "blue", "value": null }
            ]
          }
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Auth Endpoints Latency (P95)",
      "id": 5,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{uri=~\"/auth/.*\"}[$__interval])) by (le, uri))",
          "legendFormat": "{{uri}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 0.5 },
              { "color": "red", "value": 1 }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Storage Operations Latency (P95)",
      "id": 6,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_server_requests_seconds_bucket{uri=~\"/storage/.*\"}[$__interval])) by (le, method))",
          "legendFormat": "{{method}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": null },
              { "color": "orange", "value": 3 },
              { "color": "red", "value": 5 }
            ]
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Sync/Auth Request Rate",
      "id": 7,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/auth/sync\"}[$__interval]))",
          "legendFormat": "/auth/sync"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/auth/profile\"}[$__interval]))",
          "legendFormat": "/auth/profile"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/auth/favorites.*\"}[$__interval]))",
          "legendFormat": "/auth/favorites"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Storage Upload/Delete Rate",
      "id": 8,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/storage/upload\", method=\"POST\"}[$__interval]))",
          "legendFormat": "Uploads"
        },
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/storage/.*\", method=\"DELETE\"}[$__interval]))",
          "legendFormat": "Deletes"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps"
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Auth/Sync Errors",
      "id": 9,
      "gridPos": { "h": 8, "w": 12, "x": 0, "y": 20 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/auth/.*\", status=~\"4..|5..\"}[$__interval])) by (status)",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    },
    {
      "type": "timeseries",
      "title": "Storage Errors",
      "id": 10,
      "gridPos": { "h": 8, "w": 12, "x": 12, "y": 20 },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "expr": "sum(rate(http_server_requests_seconds_count{uri=~\"/storage/.*\", status=~\"4..|5..\"}[$__interval])) by (status)",
          "legendFormat": "{{status}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "reqps",
          "custom": {
            "fillOpacity": 30
          }
        }
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        }
      }
    }
  ]
}
